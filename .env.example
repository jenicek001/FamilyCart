# Database
POSTGRES_SERVER=db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=familycart
DATABASE_URL=postgresql://postgres:your_secure_password@db:5432/familycart

# JWT
SECRET_KEY=your_jwt_secret_key_here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# OAuth (optional)
# GOOGLE_OAUTH_CLIENT_ID=your_google_client_id
# GOOGLE_OAUTH_CLIENT_SECRET=your_google_client_secret

# Uncomment and configure these if using Apple OAuth
# APPLE_OAUTH_CLIENT_ID=your_apple_client_id
# APPLE_OAUTH_TEAM_ID=your_apple_team_id
# APPLE_OAUTH_KEY_ID=your_apple_key_id
# APPLE_OAUTH_PRIVATE_KEY=your_apple_private_key

# Uncomment if using pgAdmin
# PGADMIN_DEFAULT_EMAIL=admin@example.com
# PGADMIN_DEFAULT_PASSWORD=admin
